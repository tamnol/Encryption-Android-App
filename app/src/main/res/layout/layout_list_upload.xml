<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_CoordinatesUp"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lucientamno.encryptionphotosapp.PhotosActivityUpLoad">


    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbarID1"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="?attr/colorPrimary"
        app:popupTheme="@style/AppTheme.AppBarOverlay"></android.support.v7.widget.Toolbar>

        <include layout="@layout/camera" />


    <Button
        android:id="@+id/button_send_photo_id"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="145dp"
        android:layout_marginTop="300dp"
        android:layout_marginEnd="75dp"
        android:text="@string/buttonImage"
        android:textStyle="bold|italic" />

    <Button
        android:id="@+id/button_stop_cam_id"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="145dp"
        android:layout_marginTop="400dp"
        android:layout_marginEnd="75dp"
        android:text="@string/stop_cam"
        android:textStyle="bold|italic" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/back_mainActivity_id"
        android:layout_width="26dp"
        android:layout_height="26dp"
        android:layout_gravity="top|start"
        android:src="@drawable/ic_keyboard_backspace_black_24dp"
        android:textStyle="bold"
        app:backgroundTint="@android:color/darker_gray" />


</android.support.design.widget.CoordinatorLayout>